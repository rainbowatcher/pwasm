{
    "prettier.enable": false,
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
    },
    "eslint.validate": [
        "javascript",
        "typescript",
        "yaml",
        "json",
        "jsonc",
        "markdown",
    ],
    "cSpell.words": [
        "binaryen",
        "defu",
        "Deno",
        "dtolnay",
        "ofetch",
        "pwasm",
        "smol",
        "taiki",
        "unconfig",
        "wasmup",
    ],
}
