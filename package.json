{
    "name": "wasmup",
    "type": "module",
    "version": "0.5.0",
    "packageManager": "pnpm@9.6.0",
    "description": "package util for wasm",
    "author": "rainbowatcher <rainbow-w@qq.com>",
    "license": "MIT",
    "homepage": "https://github.com/rainbowatcher/wasmup#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rainbowatcher/wasmup.git"
    },
    "bugs": {
        "url": "https://github.com/rainbowatcher/wasmup/issues"
    },
    "keywords": [
        "webassembly",
        "wasm",
        "web"
    ],
    "sideEffects": false,
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "wasmup": "./dist/cli.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "lint": "eslint .",
        "prepare": "husky",
        "changelog": "conventional-changelog -si CHANGELOG.md",
        "release": "bumpp --all -x 'pnpm changelog' && pnpm build && npm publish",
        "start": "tsx src/cli.ts",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@antfu/install-pkg": "^0.3.3",
        "@clack/core": "^0.3.4",
        "cac": "^6.7.14",
        "debug": "^4.3.6",
        "dedent": "^1.5.3",
        "defu": "^6.1.4",
        "execa": "^9.3.0",
        "is-executable": "^2.0.1",
        "is-unicode-supported": "^2.0.0",
        "lookpath": "^1.2.2",
        "ofetch": "^1.3.4",
        "picocolors": "^1.0.1",
        "rimraf": "^6.0.1",
        "sisteransi": "^1.0.5",
        "smol-toml": "^1.3.0",
        "tar": "^7.4.3",
        "unconfig": "^0.5.5"
    },
    "devDependencies": {
        "@rainbowatcher/eslint-config": "^0.12.0",
        "@rainbowatcher/eslint-config-ignore": "^0.12.0",
        "@rainbowatcher/eslint-config-js": "^0.12.0",
        "@rainbowatcher/eslint-config-json": "^0.12.0",
        "@rainbowatcher/eslint-config-md": "^0.12.0",
        "@rainbowatcher/eslint-config-prettier": "^0.12.0",
        "@rainbowatcher/eslint-config-ts": "^0.12.0",
        "@types/debug": "^4.1.12",
        "@types/node": "^22.0.2",
        "@vitest/coverage-v8": "^2.0.5",
        "bumpp": "^9.4.1",
        "commitlint": "^19.3.0",
        "conventional-changelog-cli": "^5.0.0",
        "eslint": "^9.8.0",
        "husky": "^9.1.4",
        "lint-staged": "^15.2.7",
        "tsup": "^8.2.3",
        "tsx": "^4.16.4",
        "typescript": "^5.5.4",
        "vitest": "^2.0.5"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue,json}": "eslint"
    }
}
